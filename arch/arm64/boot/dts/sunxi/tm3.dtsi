			<&clk_pll_periph1x2>,
			<&clk_sdmmc1_mod>,
			<&clk_sdmmc1_bus>,
			<&clk_sdmmc1_rst>;
	clock-names = "osc24m","pll_periph","mmc","ahb","rst";
	pinctrl-names = "default","sleep";
	pinctrl-0 = <&sdc1_pins_a>;
	pinctrl-1 = <&sdc1_pins_b>;
	max-frequency = <50000000>;
	bus-width = <4>;
	non-removable; 
