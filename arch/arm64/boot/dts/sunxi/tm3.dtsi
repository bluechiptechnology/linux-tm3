	sdc_tm4_sm0_freq1 = <0>;
	sdc_tm4_sm1_freq0 = <0x00000000>;
	sdc_tm4_sm1_freq1 = <0>;
	sdc_tm4_sm2_freq0 = <0x00000000>;
	sdc_tm4_sm2_freq1 = <0>;
	sdc_tm4_sm3_freq0 = <0x05000000>;
	sdc_tm4_sm3_freq1 = <0x00000005>;
	sdc_tm4_sm4_freq0 = <0x00050000>;
