	device_type = "spi0";
	reg = <0x0 0x05010000 0x0 0x1000>;
	interrupts = <GIC_SPI 10 IRQ_TYPE_LEVEL_HIGH>;
	clocks = <&clk_pll_periph0>, <&clk_spi0>;
	clock-frequency = <100000000>;
