			pinctrl-names = "default";
			pinctrl-0 = <&xr20m1172_int>;
			spi-max-frequency = <8000000>;
			reg = <0>;
			gpio-controller;
			#gpio-cells = <2>;
			//interrupt-controller;
			//#interrupt-cells = <2>;
			status = "okay";
	};
};


