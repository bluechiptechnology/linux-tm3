	interrupts = <GIC_SPI 37 0x0104>;
	clocks = <&clk_hosc>,
			<&clk_pll_periph1x2>,
			<&clk_sdmmc2_mod>,
			<&clk_sdmmc2_bus>,
			<&clk_sdmmc2_rst>;
	clock-names = "osc24m","pll_periph","mmc","ahb","rst";
	pinctrl-names = "default","sleep";
	pinctrl-0 = <&sdc2_pins_a>;
	pinctrl-1 = <&sdc2_pins_b>;
	bus-width = <8>;
	mmc-ddr-1_8v;
	//mmc-hs200-1_8v;
	//mmc-hs400-1_8v;
	non-removable;
	max-frequency = <100000000>;
	cap-sd-highspeed;
	cap-mmc-highspeed;
	cap-erase;
	mmc-high-capacity-erase-size;
