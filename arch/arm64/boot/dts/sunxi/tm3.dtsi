&pio{

	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&pinctrl_hog_hostboard>;
	//pinctrl-1 = <&pinctrl_hog_sleep &pinctrl_hog_hostboard_sleep>;
		
	hog {
		pinctrl_hog: hoggrp {
			
		};

		pinctrl_hog_hostboard: hoggrphostboard {
			
		};

		pinctrl_hog_sleep: hoggrpslp {
			
		};

		pinctrl_hog_hostboard_sleep: hoggrphostboardslp {
			
		};
	};

	pwm1_pins_0: pwm1@0 {
		allwinner,pins = "PB19";
		allwinner,function = "pwm1";
		allwinner,pname = "pwm_positive";
		allwinner,muxsel = <0x2>;
		allwinner,pull = <0x0>;
		allwinner,drive = <0xffffffff>;
		allwinner,data = <0xffffffff>;
	};

	pwm1_pins_1: pwm1@1 {
		allwinner,pins = "PB19";
		allwinner,function = "pwm1";
		allwinner,pname = "pwm_positive";
		allwinner,muxsel = <0x7>;
		allwinner,pull = <0x0>;
		allwinner,drive = <0xffffffff>;
		allwinner,data = <0xffffffff>;
	};

	spi0_pins_a: spi0@0 {
		allwinner,pins = "PC0", "PC2", "PC3";
		allwinner,pname = "spi0_sclk", "spi0_mosi", "spi0_miso";
		allwinner,function = "spi0";
		allwinner,muxsel = <4>;
		allwinner,drive = <1>;
		allwinner,pull = <0>;
	};

	spi0_pins_b: spi0@1 {
		allwinner,pins = "PC15";
		allwinner,pname = "spi0_cs0";
		allwinner,function = "spi0";
		allwinner,muxsel = <0>;
		allwinner,drive = <1>;
		allwinner,pull = <1>;	// only CS should be pulled up
	};

	spi0_pins_c: spi0@2 {
		allwinner,pins = "PC0", "PC2", "PC3", "PC15";
		allwinner,function = "io_disabled";
		allwinner,muxsel = <7>;
		allwinner,drive = <1>;
		allwinner,pull = <0>;
	};

	sdc1_pins_a: sdc1@0 {
		allwinner,pins = "PG0", "PG1", "PG2",
					"PG3", "PG4", "PG5";
		allwinner,function = "sdc1";
		allwinner,muxsel = <2>;
		allwinner,drive = <3>;
		allwinner,pull = <1>;
	};

	sdc1_pins_b: sdc1@1 {
		allwinner,pins = "PG0", "PG1", "PG2",
					"PG3", "PG4", "PG5";
		allwinner,function = "io_disabled";
		allwinner,muxsel = <7>;
		allwinner,drive = <1>;
		allwinner,pull = <1>;
	};
};


&r_pio{
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&rpio_pinctrl_hog_hostboard>;
	//pinctrl-1 = <&pinctrl_hog_sleep &pinctrl_hog_hostboard_sleep>;
		
	hog {
		rpiopinctrl_hog: rpiohoggrp {
			
		};

		rpio_pinctrl_hog_hostboard: rpiohoggrphostboard {
			
		};

	};
	xr20m1172_int: xr20m1172_int@0 
	{
		allwinner,pins = "PM4";
		allwinner,function = "irq";
		allwinner,muxsel = <0x6>;
		allwinner,drive = <0xffffffff>;
		allwinner,pull = <0xffffffff>;
	};


	wilink_int: wilink_int@0 
	{
		allwinner,pins = "PM3";
		allwinner,function = "irq";
		allwinner,muxsel = <0x6>;
		allwinner,drive = <0xffffffff>;
		allwinner,pull = <0xffffffff>;
	};
	

};



